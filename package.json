{
  "name": "starter-nuxt3",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@9.10.0",
  "description": "Nuxt 3 SSR & full-stack starter template, supports I18n, UnoCSS, Prisma, MySQL and provides better SEO.",
  "author": "Kieran Wong <kieranwv@gmail.com> (https://github.com/kieranwv/)",
  "license": "MIT",
  "engines": {
    "pnpm": ">=9.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "postinstall": "nuxi prepare",
    "dev": "nuxi dev --host --dotenv .env.development",
    "build": "nuxi build --dotenv .env.production",
    "preview": "nuxi preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxi typecheck",
    "clean": "rimraf .output",
    "release": "bumpp"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.4",
    "@nuxtjs/i18n": "^9.1.0",
    "@pinia/nuxt": "^0.7.0",
    "@unocss/nuxt": "^0.61.9",
    "@unocss/reset": "^0.61.9",
    "@vueuse/core": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "axios": "^1.7.7",
    "nuxt": "^3.14.1592",
    "nuxt-lodash": "^2.5.3",
    "pinia": "^2.2.6",
    "unocss": "^0.61.9",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@nuxt/test-utils": "^3.14.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.1",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
