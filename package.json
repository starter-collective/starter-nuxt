{
  "name": "@templates-collective/starter-nuxt3",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.8",
  "description": "Nuxt 3 SSR starter template, supports I18n, Axios, UnoCSS, and provides better SEO.",
  "author": "Kieran Wong <kieranwong9865@gmail.com> (https://github.com/kieranwong9865/)",
  "license": "MIT",
  "engines": {
    "node": ">=18 || >=20"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "dev": "nuxt dev",
    "dev:test": "nuxt dev --mode testing",
    "dev:stag": "nuxt dev --mode staging",
    "dev:prod": "nuxt dev --mode production",
    "build:dev": "nuxt build --mode development",
    "build:test": "nuxt build --mode testing",
    "build:stag": "nuxt build --mode staging",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "release": "bumpp"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^8.3.3",
    "@pinia/nuxt": "^0.5.2",
    "@unocss/reset": "^0.61.9",
    "axios": "^1.7.3",
    "nuxt": "^3.12.4",
    "pinia": "^2.2.0",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@iconify-json/carbon": "^1.1.36",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.2",
    "@unocss/nuxt": "^0.61.9",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "eslint": "^9.8.0",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.7",
    "nuxt-lodash": "^2.5.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
