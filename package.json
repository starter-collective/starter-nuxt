{
  "name": "starter-nuxt3",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.8",
  "description": "Nuxt 3 SSR & full-stack starter template, supports I18n, UnoCSS, Prisma, MySQL and provides better SEO.",
  "author": "Kieran Wong <kieranwong9865@gmail.com> (https://github.com/kieranwong9865/)",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev --host",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "release": "bumpp"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.1",
    "@nuxtjs/i18n": "^8.5.2",
    "@pinia/nuxt": "^0.5.4",
    "@unocss/nuxt": "^0.61.9",
    "@unocss/reset": "^0.61.9",
    "@vueuse/core": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "nuxt": "^3.13.1",
    "nuxt-lodash": "^2.5.3",
    "pinia": "^2.2.2",
    "unocss": "^0.61.9",
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.4.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.10.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
